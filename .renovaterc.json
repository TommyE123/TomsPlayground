{
  "regexManagers": [
    {
      "fileMatch": ["^docker/arduino-cli/Dockerfile$"],
      "matchStrings": [
        "ARG ARDUINO_CLI_VERSION=\"(?<currentValue>\\d+\\.\\d+\\.\\d+)\\S*"
      ],
      "depNameTemplate": "arduino/arduino-cli",
      "datasourceTemplate": "github-releases",
      "extractVersionTemplate": "^(?<version>.*)$"
    }
  ]
}
